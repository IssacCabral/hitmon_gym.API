generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @unique @default(uuid())
  // email String @unique @db.Varchar(30)
  email    String @unique
  password String

  userName String
  cpf      String @unique

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model Test {
  id   String @id @unique @default(uuid())
  name String
}
